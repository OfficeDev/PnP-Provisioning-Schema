              <pnp:SharedField ID="{A5DE9600-B7A6-42DD-A05E-10D4F1500208}" />
            </pnp:SharedFields>
            <pnp:WelcomePageFields>
              <pnp:WelcomePageField ID="{23203E97-3BFE-40CB-AFB4-07AA2B86BF45}" />
              <pnp:WelcomePageField ID="{B01B3DBC-4630-4ED1-B5BA-321BC7841E3D}" />
