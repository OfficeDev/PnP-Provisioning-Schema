        CalendarType="Hebrew"
        Collation="5"
        FirstDayOfWeek="Sunday"
        FirstWeekOfYear="1"
        LocaleId="1040"
        ShowWeeks="true"
        Time24="true"
        TimeZone="4"
        WorkDayEndHour="5:00PM"
        WorkDayStartHour="9:00AM"
        WorkDays="62"
        />

