              Description="Group of Power Users"
              Owner="admin@contoso.com">
            <pnp:Members>
              <pnp:User Name="user1@contoso.com" />
              <pnp:User Name="user2@contoso.com" />
              <pnp:User Name="user3@contoso.com" />
